{
  "name": "ember-material-components-web",
  "version": "0.0.40",
  "description": "A suite of Ember components wrapping the official Google material-components-web library",
  "keywords": [
    "components",
    "design",
    "ember",
    "ember-addon",
    "google",
    "material",
    "material-components",
    "material-components-web",
    "paper",
    "ui"
  ],
  "repository": "secondstreet/ember-material-components-web",
  "license": "MIT",
  "author": "Kerrick Long",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "add-contrib": "all-contributors add",
    "build": "ember build",
    "precommit": "lint-staged",
    "generate-contrib": "all-contributors generate",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --list-different {{addon,app,src,tests,config,blueprints}/**/*.{js,ts,css,json},*.{js,json}}",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.0",
    "ember-rollup": "^1.0.0",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@material/animation": "0.34.0",
    "@material/base": "0.35.0",
    "@material/button": "0.37.1",
    "@material/card": "0.2.0",
    "@material/checkbox": "0.23.0",
    "@material/drawer": "0.5.9",
    "@material/elevation": "^0.36.1",
    "@material/fab": "0.3.16",
    "@material/form-field": "0.2.14",
    "@material/grid-list": "0.2.13",
    "@material/icon-toggle": "0.1.19",
    "@material/layout-grid": "0.4.4",
    "@material/linear-progress": "0.1.8",
    "@material/list": "0.2.17",
    "@material/menu": "0.4.5",
    "@material/radio": "0.2.12",
    "@material/ripple": "^0.37.1",
    "@material/selection-control": "^0.37.1",
    "@material/switch": "0.1.12",
    "@material/tabs": "0.3.0",
    "@material/textfield": "0.3.6",
    "@material/theme": "^0.35.0",
    "@material/toolbar": "0.4.7",
    "@material/typography": "^0.37.1",
    "all-contributors-cli": "^4.10.1",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.2.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-cli-updater": "1.0.0",
    "ember-composable-helpers": "^2.0.3",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-freestyle": "^0.8.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "0.5.10",
    "ember-qunit-nice-errors": "1.2.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.2.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "loader.js": "^4.2.3",
    "prettier": "^1.11.1",
    "qunit-dom": "^0.6.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://secondstreet.github.io/ember-material-components-web/",
    "versionCompatibility": {
      "ember": ">=2.10.0"
    }
  },
  "lint-staged": {
    "linters": {
      "{{addon,app,src,tests,config,blueprints}/**/*.{js,ts,css,json},*.{js,json}}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
